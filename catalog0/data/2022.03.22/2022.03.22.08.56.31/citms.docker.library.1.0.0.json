  "description": "Pull docker image without docker installation\n        public async Task<ApiResult<string>> Test([FromServices] IImagePuller imagePuller, [FromServices] IImageFactory imageFactory)\n        {\n            var result = new ApiResult<string>();\n            try\n            {\n                var image = await imageFactory.CreateFromExpressionAsync(\"reg.citms.cn:7443/vdp-arm64/vdpw:20220320-224716\");\n                var xx =  Path.Combine(AppContext.BaseDirectory, \"TempFiles\");\n                await imagePuller.PullAsync(image, xx);\n            }\n            catch (Exception e)\n            {\n\n                result.Code = ResultCode.Parameter_Error;\n                result.Message = e.ToString();\n            }\n            return result;\n        }",
