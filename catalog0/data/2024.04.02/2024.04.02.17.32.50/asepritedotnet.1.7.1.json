  "deprecation": {
    "@id": "https://api.nuget.org/v3/catalog0/data/2024.04.02.17.32.50/asepritedotnet.1.7.1.json#deprecation",
    "message": "This version has a critical bug due to how Aseprite stores pixel data for a cel outside the bounds of a frame. If you select part of your image and move the pixels outside of the frame, the pixel data for the cel is still there even though its no longer visible or within the frame bounds. The method originally used to detect this was not sufficient enough, and has been adjusted to explicitly only process pixel data of a cel that is within the bounds of a frame, regardless of the cel size.",
    "reasons": [
      "CriticalBugs"
    ]
  },
