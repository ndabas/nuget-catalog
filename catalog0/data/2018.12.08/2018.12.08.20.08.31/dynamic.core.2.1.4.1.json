  "description": "增加动态加载外部dll支持\n public static class AppDomainExtension\n    {\n        public static void InitLoadAssembly(this AppDomain appDomain, string dynamicLibFullDir)\n        {\n            AssemblyFileResolveWrap assemblyFileResolveWrap = new AssemblyFileResolveWrap(dynamicLibFullDir);\n            assemblyFileResolveWrap.InitLoadAssembly(appDomain);\n        }\n        public static Assembly LoadAssembly(this string loadDllPath)\n        {\n            var dependAssembly = Assembly.LoadFrom(loadDllPath);\n            return dependAssembly;\n        }\n    }",
