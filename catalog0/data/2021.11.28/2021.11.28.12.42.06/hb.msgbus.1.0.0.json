  "description": "观察者模式，发布&订阅，跨线程，跨窗体通信。\n   MsgBus.Subscribe<log>(str => { Console.WriteLine($\"收到：{str}\"); });\n            MsgBus.SubscribeById<log>(\"haibao\", str => { Console.WriteLine($\"haibao收到：{str}\"); });\n            MsgBus.SubscribeByIdGroup<log>(null, \"hb\", str => { Console.WriteLine($\"hb组收到：{str}\"); });\n            MsgBus.SubscribeByIdGroup<log>(\"haibao2\", \"hb\", str => { Console.WriteLine($\"hb组haibao2订阅收到：{str}\"); });\n            MsgBus.Subscribe<GetName>(() => { return \"hhh\"; });\n            MsgBus.Publish<log>(t => t(\"这是一个广播\"));\n            MsgBus.PublishByGroupId<log>(null, \"haibao\", t => t(\"发给haibao的信息\"));\n            MsgBus.PublishByGroupId<log>(\"hb\", null, t => t(\"发给hb组的信息\"));\n            MsgBus.ChangeGroup(\"haibao2\", \"hb3\");\n            MsgBus.PublishByGroupId<log>(\"hb3\", null, t => t(\"发给hb3组的信息\"));\n            MsgBus.ClearSubscription<log>(\"haibao\");\n            MsgBus.PublishByGroupId<log>(\"hb\", null, t => t(\"发给hb组的信息\"));\n            MsgBus.PublishAsyncForResult<GetName, string>(t => t(), list =>\n            {\n                Console.WriteLine(list[0]);\n            });",
