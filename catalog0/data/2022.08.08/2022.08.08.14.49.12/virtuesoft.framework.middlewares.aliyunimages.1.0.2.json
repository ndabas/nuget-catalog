  "releaseNotes": "public void ConfigureServices(IServiceCollection services)\n        {\n            var config = Configuration.GetSection(\"aliyunoss\");\n            services.AddAliyunImage(option => {\n                option.AccessEndpoint = config[\"AccessEndpoint\"];\n                option.AccessKeyId = config[\"AccessKeyId\"];\n                option.AccessKeySecret = config[\"AccessKeySecret\"];\n                option.BucketName = config[\"BucketName\"];\n                option.Endpoint = config[\"Endpoint\"];\n                option.Folder = config[\"Folder\"];\n            });\n        }\n\n        public void Configure(IApplicationBuilder app, IWebHostEnvironment env)\n        {\n            app.UseAliyunImages();\n        }",
