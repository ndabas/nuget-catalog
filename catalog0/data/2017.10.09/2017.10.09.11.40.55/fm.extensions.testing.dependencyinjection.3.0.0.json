  "description": "Provides a base class for tests based on Microsoft.Extensions.DependencyInjection. Use common Startup.cs patterns like ConfigureServices(...) and Configure(...) to setup your tests.\n\nCommonly used types:\nServiceTestsBase\n\nExample:\n[TestClass]\npublic class DatabaseCleanupBackgroundServiceTests : ServiceTestsBase\n{\n    protected override void ConfigureServices([NotNull] IServiceCollection services)\n    {\n        base.ConfigureServices(services);\n\n        services.AddSingleton<DatabaseCleanupBackgroundService>();\n        services.AddSingleton(sp => A.Fake<IDatabaseCleanupService>());\n        services.AddSingleton<ILogRecorder, LogRecorderProvider>();\n    }\n\n    protected override void Configure([NotNull] IServiceProvider services, [NotNull] ILoggerFactory loggerFactory)\n    {\n        base.Configure(services, loggerFactory);\n        loggerFactory.AddLogRecorder(services);\n    }\n\n    [TestMethod]\n    public void CleanupSuggestedFailTest()\n    {\n        IDatabaseCleanupService databaseCleanupService = this.GetRequiredService<IDatabaseCleanupService>();\n        A.CallTo(() => databaseCleanupService.RemoveObsoleteProjectsAsync(A<CancellationToken>.Ignored)).Throws(new InvalidOperationException(nameof(this.CleanupSuggestedFailTest)));\n\n        DatabaseCleanupBackgroundService service = this.GetRequiredService<DatabaseCleanupBackgroundService>();\n        service.CleanupSuggested();\n\n        service.WaitAll();\n\n        ILogRecorder logRecorder = this.GetRequiredService<ILogRecorder>();\n        logRecorder.Messages.Single(m => m.LogLevel == LogLevel.Error).Exception.ShouldBeOfType<InvalidOperationException>().Message.ShouldBe(nameof(this.CleanupSuggestedFailTest));\n    }\n}",
